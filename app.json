{
  "expo": {
    "name": "KeepTouch",
    "slug": "bolt-expo-nativewind",
    "version": "1.1.6",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "touchbasebolt",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f8f5ed"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/icon.png",
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"],
        "NSContactsUsageDescription": "Allow Keep Connect to access your contacts.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.holygrailstudio.boltexponativewind"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#f8f5ed"
      },
      "permissions": [
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS",
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS"
      ],
      "package": "com.holygrailstudio.boltexponativewind"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow Keep Connect to access your contacts."
        }
      ],
      "expo-notifications",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "hgs-ae"
        }
      ],
      "expo-updates"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f378a217-f470-4517-90db-7bb1b65a13a1"
      },
      "stripeMonthlyPriceId": "price_1RGnMkBaBfFAgOwMe1ABL45G",
      "stripeYearlyPriceId": "price_1RGnLqBaBfFAgOwMRU7SuiGA",
      "SENTRY_DSN": "REPLACE_WITH_YOUR_SENTRY_DSN_HERE"
    },
    "newArchEnabled": false
  }
}
